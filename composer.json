    "description": "OpenAPI API testing in Laravel projects.",
    "homepage": "https://github.com/p0n0marev/laravel-openapi-validate",
    "keywords": ["laravel", "testing", "phpunit", "openapi"],
    "require": {
        "php": ">=7.2",
    },
    "require-dev": {
        "phpunit/phpunit": "^7.0.0",
        "league/openapi-psr7-validator": "^0.15.2",
